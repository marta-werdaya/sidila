<!-- Bagian Tabel Disability -->
<div class="row" id="target">
  <div class="col-12">
    <div class="card">
      <div class="card-header">
        <h4 id="header">
            <!-- <button class=" btn btn-primary " id="pdf">PDF</button> -->
        </h4>
        <div class="card-header-form">
          <form>
            <div class="input-group">
              <input type="text" class="form-control" placeholder="Search">
              <div class="input-group-btn">
                <button class="btn btn-primary"><i class="fas fa-search"></i></button>
              </div>
            </div>
          </form>
        </div>
      </div>
      <div class="card-body p-0">
        <div class="table-responsive">
          <table class="table table-striped table-bordered" id="table-data">
            <tr>
              <th>No</th>
              <th>nama</th>
              <th>tanggal pengajuan</th>
              <th>tanggal penerimaan</th>
              <th>Pengajuan</th>
              <th>Pemberi Bantuan</th>
              <th>penerima</th>
            </tr>
            <% for(i = 0; i < penerimas.length; i++) {%>
              <% if(penerimas[i].proposalId.name != "Pengajuan Lansia"){ %>
                <% const datediterima = penerimas[i].receiptDate.toISOString().split('T')[0]%>
                <% const datepengajuan = penerimas[i].proposalId.dateApply.toISOString().split('T')[0]%>
            <tr>
              <td><%= i + 1 %></td>
              <td><%= penerimas[i].proposalId.personId.name%></td>
              <td><%= datepengajuan %></td>
              <td><%= datediterima%></td>
              <td><%= penerimas[i].proposalId.name%></td>
              <td><%= penerimas[i].jenisBantuan.pemberi%></td>
              <td><%= penerimas[i].penerima%></td>
              <td>
                <!-- <div class="dropdown">
                  <a href="#" data-toggle="dropdown" class="btn btn-outline-info dropdown-toggle">Pilihan</a>
                  <div class="dropdown-menu">
                    <a href="javascript:void()" class="dropdown-item has-icon button-edit"
                      data-id="<%= penerimas[i].id %>" data-name="<%= penerimas[i].name %>"><i
                        class="fas fa-edit"></i>
                      Edit</a>
                    <div class="dropdown-divider"></div>
                    <a href="javascript:void()" class="dropdown-item has-icon text-danger button-delete"
                      data-id="<%= penerimas[i].id %>"><i class="fas fa-trash-alt"></i>
                      Hapus</a>
                  </div>
                </div> -->
              </td>
            </tr>
            <% } %>
            <% } %>
          </table>
        </div>
      </div>
      <!-- Akhir Tabel -->
      <!-- Tabel Pagination -->
      <div class="card-footer text-right">
        <nav class="d-inline-block">
          <ul class="pagination mb-0">
            <li class="page-item disabled">
              <a class="page-link" href="#" tabindex="-1"><i class="fas fa-chevron-left"></i></a>
            </li>
            <li class="page-item active"><a class="page-link" href="#">1 <span class="sr-only">(current)</span></a></li>
            <li class="page-item"><a class="page-link" href="#">2</a></li>
            <li class="page-item"><a class="page-link" href="#">3</a></li>
            <li class="page-item">
              <a class="page-link" href="#"><i class="fas fa-chevron-right"></i></a>
            </li>
          </ul>
        </nav>
      </div>
      <!-- Tabel Pagination -->
    </div>
  </div>
</div>